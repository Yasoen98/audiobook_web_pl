{
  "name": "backend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsup src/index.ts --format esm --dts --out-dir dist",
    "start": "node dist/index.js",
    "lint": "eslint 'src/**/*.ts'",
    "test": "vitest run",
    "migrate": "prisma migrate deploy",
    "migrate:dev": "prisma migrate dev",
    "generate": "prisma generate",
    "seed": "tsx src/scripts/seed.ts",
    "format": "prettier --write 'src/**/*.ts'"
  },
  "dependencies": {
    "@fastify/cookie": "^10.1.1",
    "@fastify/cors": "^10.0.1",
    "@fastify/jwt": "^7.2.5",
    "@fastify/rate-limit": "^8.0.1",
    "@fastify/sensible": "^5.4.0",
    "@fastify/static": "^6.11.2",
    "@polski-lektor-ai/shared": "workspace:*",
    "@prisma/client": "^5.9.1",
    "@sinclair/typebox": "^0.32.28",
    "argon2": "^0.31.1",
    "bullmq": "^4.12.2",
    "fastify": "^4.26.2",
    "fastify-helmet": "^11.1.1",
    "fastify-multipart": "^7.7.4",
    "fastify-plugin": "^4.5.1",
    "fastify-rate-limit": "^8.0.0",
    "fastify-swagger": "^8.12.0",
    "file-type": "^18.5.0",
    "jsonwebtoken": "^9.0.2",
    "mime-types": "^2.1.35",
    "node-fetch": "^3.3.2",
    "pdf-parse": "^1.1.1",
    "pg": "^8.11.3",
    "pino": "^8.17.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bull": "^3.15.9",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.24",
    "@types/pdf-parse": "^1.1.3",
    "@types/ws": "^8.5.8",
    "eslint": "^8.56.0",
    "prisma": "^5.9.1",
    "prettier": "^3.2.5",
    "tsx": "^4.7.1",
    "tsup": "^7.2.0",
    "typescript": "^5.3.3",
    "vitest": "^1.3.1"
  }
}
